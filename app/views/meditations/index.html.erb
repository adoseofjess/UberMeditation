<div class="container">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">Past Meditations</h3>
    </div>
    <div class="panel-body">
      <table class="table table-hover">
        <tr>
          <th>Start</th>
          <th>End</th>
          <th>Notes</th>
        </tr>
        <% @meditations.each do |meditation| %>
          <tr>
            <td><%= meditation.start_time %></td>
            <td><%= meditation.end_time %></td>
            <td><%= meditation.post_notes %></td>
          </tr>
        <% end %>
      </table>
  </div>
</div>